#!/usr/bin/env bash

# This script helps the user to make sure changes to firstlife get committed and pushed

. $SKIGGETY_UTILS_DIR/lib/skiggety-utils.bash || exit 1
. $FIRSTLIFE_CODE_DIR/lib/firstlife.bash || exit_with_error "could not source firstlife.bash"
# TODO: for when we move firstlife.bash over: . $ZEROTHLIFE_CODE_DIR/lib/zerothlife.bash || exit_with_error "could not source zerothlife.bash"

cd $FIRSTLIFE_CODE_DIR && git-clean-up-local-changes
