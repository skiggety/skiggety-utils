#!/usr/bin/env bash

THIS_DIR="$(cd "$(dirname $BASH_SOURCE)";pwd)"
. $THIS_DIR/../lib/skiggety-utils.bash || exit 1

if [ -z "$SKIGGETY_UTILS_DIR" ]; then
    exit_with_error "Please add \"export=SKIGGETY_UTILS_DIR='$(dirname $THIS_DIR)'\" to the end of your ~/.bashrc file, then restart bash and try this script again"
fi
